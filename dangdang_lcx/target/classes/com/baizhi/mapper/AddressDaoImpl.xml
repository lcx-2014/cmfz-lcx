<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.AddressDao">
  	<select id="selectByUserId" resultType="com.baizhi.entity.Address">
  	select * from daddress where userId=#{id}
  	</select>
  	<select id="selectByAddId" resultType="com.baizhi.entity.Address">
  	select * from daddress where id=#{addId}
  	</select>
</mapper>
